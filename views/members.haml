%h2= @members.count

- @members.each do | member |

  #member
    %img{ :src => "/JenniferC.jpg", :height => 128, :width => "128"}

  #content
    %h2 Name:
    %p= member.name
    %h2 Title:
    %p= member.title
    %h2 Location:
    %p= member.location
    %h2 Vertical:
    %p= member.vertical
    -#%h2 Anniversary:
    -#%p= member.anniversary
    %h2 Bio:
    %p= member.bio
    %a{:href => "/members/#{member.id}/edit", :class => "button"} edit
